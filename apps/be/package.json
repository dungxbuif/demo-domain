{
   "name": "qn-official-api",
   "version": "1.0.0",
   "description": "Qn Official Api",
   "author": "",
   "private": true,
   "license": "MIT",
   "scripts": {
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "nest start",
      "dev": "nest start --debug  --watch",
      "start:bot": "nest start --entryFile bot/main",
      "dev:bot": "nest start --debug=9230 --watch --entryFile bot/main",
      "typeorm": "typeorm-ts-node-commonjs",
      "migrate": "npm run typeorm -- migration:run -d ormconfig.ts",
      "migration:generate": "npm run typeorm -- migration:generate ./migrations/$npm_config_name -d ormconfig.ts",
      "migration:create": "npm run typeorm -- migration:create ./migrations/$npm_config_name",
      "migration:revert": "npm run typeorm -- migration:revert -d ormconfig.ts",
      "seed": "ts-node -r tsconfig-paths/register ./src/seed.ts",
      "schema:drop": "yarn run typeorm schema:drop",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "lint": "eslint . --ext .ts",
      "lint:fix": "eslint --fix . --ext .ts"
   },
   "dependencies": {
      "@date-fns/tz": "^1.4.1",
      "@nestjs/common": "^10.3.0",
      "@nestjs/config": "^3.1.1",
      "@nestjs/core": "^10.3.0",
      "@nestjs/event-emitter": "^3.0.1",
      "@nestjs/jwt": "^10.2.0",
      "@nestjs/passport": "^11.0.5",
      "@nestjs/platform-express": "^10.3.0",
      "@nestjs/schedule": "^6.0.1",
      "@nestjs/swagger": "^7.1.17",
      "@nestjs/typeorm": "^10.0.1",
      "@qnoffice/shared": "file:../../libs",
      "@sendgrid/mail": "^8.1.3",
      "@types/cookie-parser": "^1.4.10",
      "@types/uuid": "^11.0.0",
      "axios": "^1.7.7",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.1",
      "compression": "^1.7.4",
      "cookie-parser": "^1.4.7",
      "date-fns": "^4.1.0",
      "eventemitter2": "^6.4.9",
      "express": "^4.18.2",
      "express-http-context": "^2.0.1",
      "helmet": "^7.1.0",
      "jsonwebtoken": "^9.0.2",
      "lodash": "^4.17.21",
      "mezon-sdk": "^2.8.33",
      "mime-types": "^2.1.35",
      "minimatch": "^10.0.3",
      "morgan": "^1.10.0",
      "ms": "^2.1.3",
      "nestjs-cls": "^3.6.0",
      "nestjs-i18n": "^10.4.0",
      "parse-duration": "^1.1.0",
      "passport": "^0.6.0",
      "passport-jwt": "^4.0.1",
      "pg": "^8.11.3",
      "query-string": "^9.0.0",
      "reflect-metadata": "^0.2.1",
      "rxjs": "^7.8.1",
      "source-map-support": "^0.5.21",
      "swagger-ui-express": "^5.0.0",
      "tslib": "^2.6.2",
      "typeorm": "^0.3.19",
      "typeorm-transactional": "~0.5.0",
      "uuid": "^13.0.0"
   },
   "devDependencies": {
      "@nestjs/cli": "^10.2.1",
      "@nestjs/schematics": "^10.1.4",
      "@nestjs/testing": "^10.3.0",
      "@types/axios": "^0.14.0",
      "@types/compression": "^1.7.5",
      "@types/express": "^4.17.21",
      "@types/jest": "^29.5.11",
      "@types/jsonwebtoken": "^9.0.5",
      "@types/lodash": "^4.14.202",
      "@types/mime-types": "^2.1.4",
      "@types/morgan": "^1.9.9",
      "@types/ms": "^0.7.34",
      "@types/node": "^20.10.6",
      "@types/passport-jwt": "^3.0.13",
      "@types/supertest": "^2.0.16",
      "@types/uuid": "^9.0.7",
      "@typescript-eslint/eslint-plugin": "^6.17.0",
      "@typescript-eslint/parser": "^6.17.0",
      "cross-env": "^7.0.3",
      "eslint": "^8.56.0",
      "eslint-config-prettier": "^9.1.0",
      "eslint-import-resolver-typescript": "^3.6.1",
      "eslint-plugin-import": "^2.29.1",
      "eslint-plugin-import-helpers": "^1.3.1",
      "eslint-plugin-n": "^16.6.1",
      "eslint-plugin-no-relative-import-paths": "^1.5.4",
      "eslint-plugin-no-secrets": "^1.0.2",
      "eslint-plugin-prettier": "^5.1.2",
      "eslint-plugin-sonarjs": "^0.23.0",
      "eslint-plugin-unicorn": "^50.0.1",
      "jest": "^29.7.0",
      "lint-staged": "~15.2.0",
      "prettier": "^3.1.1",
      "supertest": "^6.3.3",
      "ts-jest": "^29.1.1",
      "ts-node": "^10.9.2",
      "tsconfig-paths": "^4.2.0",
      "typescript": "^5.3.3"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "roots": [
         "<rootDir>/src",
         "<rootDir>/__tests__"
      ],
      "setupFilesAfterEnv": [
         "<rootDir>/__tests__/setup.ts"
      ],
      "testRegex": ".spec.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "coverageDirectory": "../coverage",
      "testEnvironment": "node",
      "moduleNameMapper": {
         "^@src/(.*)$": "<rootDir>/src/$1",
         "^@qnoffice/shared$": "<rootDir>/../../libs/src/index.ts"
      },
      "testTimeout": 30000
   }
}